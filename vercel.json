{
  "version": 2,
  "github": {
    "enabled": true
  },
  "builds": [
    {
      "src": "/apps/api/src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/api/dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/$1"
    }
  ]
}
