{
    "version": 2,
    "github": {
      "enabled":true
    },
    "builds": [
      {
        "src": "/apps/api/src/app.ts",  
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/apps/api/src/app.ts" ,
        "methods": ["POST"] 
      }
    ]
  }
  