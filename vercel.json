{
  "version": 2,
  "builds": [
    {
      "src": "server/dist/app.js", 
      "use": "@vercel/node"        
    },
    {
      "src": "web/next.config.js",  
      "use": "@vercel/next"      
    }
  ],
  "routes": [
    {
      "src": "/server/v1/(.*)",  
      "dest": "/server/dist/app.js"
    },
    {
      "src": "/(.*)",           
      "dest": "/web/$1"
    }
  ]
}
